{
  "name": "sia_challenge_2025",
  "version": "1.0.0",
  "description": "a websocket app to show and coordinate content between clients",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "node dist/server.js",
    "dev": "node --watch src/server.js",
    "build": "node scripts/build.js",
    "build:watch": "vite build --watch --config vite.config.js & vite build --watch --config vite.server.config.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "sia",
    "challenge"
  ],
  "author": "ppicapietra",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.46.0",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.54.0",
    "supertest": "^6.3.3",
    "vite": "^5.4.21",
    "vitest": "^1.0.4"
  }
}
